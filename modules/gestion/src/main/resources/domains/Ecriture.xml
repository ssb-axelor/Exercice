<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">

  <module name="gestion" package="com.axelor.gestion.db"/>

  
  	<entity name="Ecriture" sequential="true" cachable="true">
  		<date name="dateOperation" required="true"/>       
  		<string name="referencePiece" required="true"/>
  		<one-to-many name="ecritures" ref="LigneEcriture" mappedBy="ecriture"/>
	</entity>
  
</domain-models>
